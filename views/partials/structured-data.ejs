<!-- Structured Data -->
<script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "CleaningService",
    "name": "<%= businessName %>",
    "description": "<%= description %>",
    "image": "<%= logoUrl %>",
    "url": "<%= canonical %>",
    "telephone": "<%= phone %>",
    "address": {
      "@type": "PostalAddress",
      "streetAddress": "100 Chesterfield Business Pkwy 2nd floor ste 255",
      "addressLocality": "Chesterfield",
      "addressRegion": "MO",
      "postalCode": "63005",
      "addressCountry": "US"
    },
    "areaServed": {
      "@type": "GeoCircle",
      "geoMidpoint": {
        "@type": "GeoCoordinates",
        "latitude": 38.6631,
        "longitude": -90.5771
      },
      "geoRadius": 50000
    },
    "openingHours": "Mo,Tu,We,Th,Fr 08:00-17:00",
    "priceRange": "$$",
    "sameAs": ["https://www.facebook.com/cleanpro", "https://www.twitter.com/cleanpro", "https://www.instagram.com/cleanpro"]
  }
